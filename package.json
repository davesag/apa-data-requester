  "scripts": {
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js  --recursive",
    "test:debug": "npm run test -- --debug-brk",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/davesag/apa-data-requester.git"
  },
  "keywords": [
    "redux",
