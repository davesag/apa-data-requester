  "license": "MIT",
  "bugs": {
    "url": "https://github.com/davesag/apa-data-requester/issues"
  },
  "homepage": "https://github.com/davesag/apa-data-requester#readme",
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-preset-es2015": "^6.6.0",
    "chai": "^3.5.0",
    "chai-immutable": "^1.5.4",
    "faker": "^3.1.0",
    "mocha": "^2.4.5"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
